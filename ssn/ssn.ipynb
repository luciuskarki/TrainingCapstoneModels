   "id": "55d1ce97-2abd-4265-8ac1-d83e91c421af",
   "metadata": {},
   "outputs": [],
   "source": [
    "ssn_pattern = (\n",
    "    r'\\b(?!000|666|9\\d\\d)\\d{3}'\n",
    "    r'-?(?!00)\\d{2}'\n",
    "    r'-?(?!0000)\\d{4}\\b'\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "b8bb1f92-4e84-4d7b-985d-6713c360a369",
   "metadata": {},
   "outputs": [],
   "source": [
    "def is_a_ssn(token):\n",
    "    return bool(re.search(ssn_pattern, token))"
   ]
  }
