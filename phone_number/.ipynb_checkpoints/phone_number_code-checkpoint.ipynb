   "outputs": [],
   "source": [
    "phone_pattern = r'\\b(?:\\(\\d{3}\\)|\\d{3})[-.\\s]?\\d{3}[-.\\s]?\\d{4}\\b'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 142,
   "id": "9fa9cc13-9406-4407-9407-402ac96be32a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def is_a_phone_number(token):\n",
    "    return bool(re.search(phone_pattern, token))"
   ]
  }
